{
  "Applications": {
    "{{$1}}-baserow-api": {
      "Details": {
        "Description": "Baserow API",
        "Name": "Baserow API"
      },
      "LookupConfig": {
        "PathRegex": "/api/baserow.*"
      },
      "Package": {
        "Details": {
          "APIRoot": "https://api.baserow.io/api",
          "Security": "Authorization~Token {{SECURITY_TOKEN}}"
        },
        "Type": "APILowCodeUnit"
      },
      "Processor": {
        "Details": {
          "InboundPath": "api/baserow"
        },
        "Type": "ProxyProcessor"
      }
    },
    "{{$2}}-mui-baserow-carousel": {
      "Details": {
        "Description": "Baserow React Carousel",
        "Name": "Baserow React Carousel"
      },
      "LookupConfig": {
        "PathRegex": "/baserow/carousel.*"
      },
      "Package": {
        "Details": {
          "Package": "@fathym-it/mui-baserow-carousel",
          "Version": "latest"
        },
        "Type": "NPMLowCodeUnit"
      },
      "Processor": {
        "Details": {
          "BaseHref": "/baserow/carousel/",
          "DefaultFile": "index.html"
        },
        "Type": "DFSProcessor"
      }
    },
    "{{$3}}-baserow-landing-page": {
      "Details": {
        "Description": "The landing page for Baserow",
        "Name": "Baserow Home Page"
      },
      "LookupConfig": {
        "PathRegex": "/.*"
      },
      "Package": {
        "Details": {
          "Build": "latest",
          "Path": "lcu-github-plasmic-artifacts-release.yml"
        },
        "SourceControlLookup": "github://{{ORGANIZATION}}/landing-page-baserow-launchpad",
        "Type": "GitHubLowCodeUnit"
      },
      "Processor": {
        "Details": {
          "BaseHref": "/",
          "DefaultFile": "index.html"
        },
        "Type": "DFSProcessor"
      }
    }
  },
  "Environments": {
    "{{EaC.PrimaryEnvironment}}": {
      "Artifacts": {
        "plasmic-build-release": {
          "Type": "GitHubArtifact",
          "Details": {
            "Name": "Plasmic Build/Release",
            "Output": "./public",
            "BuildCommand": "npm run build",
            "InstallCommand": "npm i",
            "NPMRegistry": "https://registry.npmjs.org/"
          }
        }
      },
      "DevOpsActions": {
        "github-artifacts-release-plasmic": {
          "ArtifactLookups": ["plasmic-build-release"],
          "Details": {
            "Name": "GitHub Artifacts - Release - Plasmic",
            "Path": "lcu-github-plasmic-artifacts-release.yml",
            "Templates": [
              "https://raw.githubusercontent.com/lowcodeunit/infrastructure/master/github/actions/artifacts/_header.hbs.yml",
              "https://raw.githubusercontent.com/lowcodeunit/infrastructure/master/github/actions/artifacts/release-flow.yml"
            ]
          }
        }
      },
      "Sources": {
        "fork|github://fathym-it/landing-page-baserow-launchpad": {
          "DevOpsActionTriggerLookups": ["github-artifacts-release-plasmic"],
          "Details": {
            "Branches": ["main"],
            "MainBranch": "main",
            "Organization": "{{ORGANIZATION}}",
            "Repository": "landing-page-baserow-launchpad",
            "Name": "github://{{ORGANIZATION}}/landing-page-baserow-launchpad"
          },
          "Type": "GitHubSourceControl"
        }
      }
    }
  },
  "Projects": {
    "{{PROJECT_LOOKUP}}": {
      "ApplicationLookups": [
        "{{$1}}-baserow-api",
        "{{$2}}-mui-baserow-carousel",
        "{{$3}}-landing-page-baserow-launchpad"

      ]
    }
  }
}
